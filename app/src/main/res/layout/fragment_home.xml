<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/SVHome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFAEA"
    android:backgroundTintMode="multiply"
    android:elevation="4dp"
    tools:context=".HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/CLHome"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/TVHome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/inter_bold"
            android:text="@string/TVHome"
            android:textColor="#000000"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/TVHomedesc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/inter"
            android:text="@string/TVHomedesc"
            android:textColor="#787878"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TVHome" />

        <TextView
            android:id="@+id/TVLatest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/inter_bold"
            android:text="@string/TVLatest"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TVHomedesc" />

        <TextView
            android:id="@+id/TVLatest2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:fontFamily="@font/inter_bold"
            android:text="@string/TVHomeGo"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/News_Img" />

        <ImageView
            android:id="@+id/News_Img"
            android:layout_width="322dp"
            android:layout_height="173dp"
            android:layout_marginStart="1dp"
            android:src="@drawable/home_news"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TVLatest" />

        <TextView
            android:id="@+id/TVNewsdesc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="120dp"
            android:fontFamily="@font/inter_bold"
            android:text="@string/TVNewsDesc"
            android:textColor="#FFFFFF"
            app:layout_constraintStart_toStartOf="@+id/News_Img"
            app:layout_constraintTop_toTopOf="@+id/News_Img" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="26dp"
            android:layout_height="8dp"
            android:layout_marginTop="140dp"
            android:src="@drawable/news_indicator"
            app:layout_constraintEnd_toEndOf="@+id/News_Img"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="@+id/News_Img"
            app:layout_constraintTop_toTopOf="@+id/News_Img" />

        <ImageView
            android:id="@+id/calculator_btn"
            android:layout_width="322dp"
            android:layout_height="75dp"
            android:clickable="true"
            android:src="@drawable/calculator_btn"
            android:tint="#7C0027FF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TVLatest2"
            tools:ignore="UseAppTint,SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/TVCalcBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/inter"
            android:text="@string/calculator"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/calculator_btn"
            app:layout_constraintStart_toStartOf="@+id/calculator_btn"
            app:layout_constraintTop_toTopOf="@+id/calculator_btn" />

        <ImageView
            android:id="@+id/calc_arrow"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginEnd="15dp"
            android:src="@drawable/calculator_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/calculator_btn"
            app:layout_constraintEnd_toEndOf="@+id/calculator_btn"
            app:layout_constraintTop_toTopOf="@+id/calculator_btn" />

        <ImageView
            android:id="@+id/planner_btn"
            android:layout_width="322dp"
            android:layout_height="75dp"
            android:clickable="true"
            android:src="@drawable/planner_btn"
            android:tint="#9242A53A"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/calculator_btn"
            tools:ignore="UseAppTint,SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/TVPlanBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/inter"
            android:text="@string/planner"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/planner_btn"
            app:layout_constraintStart_toStartOf="@+id/planner_btn"
            app:layout_constraintTop_toBottomOf="@+id/calculator_btn" />

        <ImageView
            android:id="@+id/plan_arrow"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginEnd="15dp"
            android:src="@drawable/planner_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/planner_btn"
            app:layout_constraintEnd_toEndOf="@+id/planner_btn"
            app:layout_constraintTop_toBottomOf="@+id/calculator_btn" />

        <ImageView
            android:id="@+id/tracker_btn"
            android:layout_width="322dp"
            android:layout_height="75dp"
            android:clickable="true"
            android:src="@drawable/tracker_btn"
            android:tint="#88D65858"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/planner_btn"
            tools:ignore="UseAppTint,SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/TVTrackBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/inter"
            android:text="@string/tracker"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/tracker_btn"
            app:layout_constraintStart_toStartOf="@+id/tracker_btn"
            app:layout_constraintTop_toBottomOf="@+id/planner_btn" />

        <ImageView
            android:id="@+id/tracker_arrow"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginEnd="15dp"
            android:src="@drawable/tracker_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/tracker_btn"
            app:layout_constraintEnd_toEndOf="@+id/tracker_btn"
            app:layout_constraintTop_toBottomOf="@+id/planner_btn" />

        <ImageView
            android:id="@+id/coach_btn"
            android:layout_width="322sp"
            android:layout_height="75dp"
            android:clickable="true"
            android:src="@drawable/coaching_btn"
            android:tint="#8A5B3CB5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tracker_btn"
            tools:ignore="UseAppTint,SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/TVCoachBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/inter"
            android:text="@string/coaching"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/coach_btn"
            app:layout_constraintStart_toStartOf="@+id/coach_btn"
            app:layout_constraintTop_toBottomOf="@+id/tracker_btn" />

        <ImageView
            android:id="@+id/coach_arrow"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/coach_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/coach_btn"
            app:layout_constraintEnd_toEndOf="@+id/coach_btn"
            app:layout_constraintTop_toBottomOf="@+id/tracker_btn" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>